{
    "name": "abramcatalyst/laravel-flutterwave",
    "description": "A comprehensive Flutterwave payment gateway package for Laravel 9.x, 10.x, 11.x, 12.x and above",
    "type": "library",
    "keywords": [
        "laravel",
        "flutterwave",
        "payment",
        "gateway",
        "rave"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Onuche Abraham",
            "email": "onucheabram@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "suggest": {
        "illuminate/database": "Required if you want to use the FlutterwaveTransaction model or migrations (^9.0|^10.0|^11.0|^12.0)"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "AbramCatalyst\\Flutterwave\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AbramCatalyst\\Flutterwave\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AbramCatalyst\\Flutterwave\\FlutterwaveServiceProvider"
            ],
            "aliases": {
                "Flutterwave": "AbramCatalyst\\Flutterwave\\Facades\\Flutterwave"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

